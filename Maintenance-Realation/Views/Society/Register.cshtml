<form method="post" asp-action="Register" asp-controller="Society">
    <h1>Society Registration</h1>
    <div class="mb-3">
        <label asp-for="Name" for="exampleInputEmail1" class="form-label">Society Name</label>
        <input asp-for="Name" type="text" class="form-control" id="name">
        <span asp-validation-for="Name" style="color:red"></span>
    </div>
    <div class="mb-3">
        <label asp-for="Address" for="exampleInputPassword1" class="form-label">Address</label>
        <input asp-for="Address" type="text" class="form-control" id="address">
        <span asp-validation-for="Address" style="color:red"></span>

    </div>
    <div class="mb-3">
        <label asp-for="PinCode" for="exampleInputPassword1" class="form-label">PinCode</label>
        <input asp-for="PinCode" type="number" class="form-control" id="pin">
        <span asp-validation-for="PinCode" style="color:red"></span>

    </div>
    <div class="mb-3">
        <label asp-for="State" for="exampleInputPassword1" class="form-label">State</label>
        <input asp-for="State" type="text" class="form-control" id="R.date">
        <span asp-validation-for="State" style="color:red"></span>

    </div>
    <div class="mb-3">
        <label asp-for="City" for="exampleInputPassword1" class="form-label">City</label>
        <input asp-for="City" type="text" class="form-control" id="builtYear">
        <span asp-validation-for="City" style="color:red"></span>

    </div>
    <div class="mb-3">
        <label asp-for="LandMark" for="exampleInputPassword1" class="form-label">LandMark</label>
        <input asp-for="LandMark" type="text" class="form-control" id="builtYear">
        <span asp-validation-for="LandMark" style="color:red"></span>

    </div>
    <div class="mb-3">
        <label asp-for="BuiltYear" for="exampleInputPassword1" class="form-label">BuiltYear</label>
        <input asp-for="BuiltYear" type="text" class="form-control" id="builtYear">
        <span asp-validation-for="BuiltYear" style="color:red"></span>

    </div>
    <div class="mb-3">
        <label asp-for="RegistrationNo" for="exampleInputPassword1" class="form-label">RegistrationNo</label>
        <input asp-for="RegistrationNo" type="text" class="form-control" id="builtYear">
        <span asp-validation-for="RegistrationNo" style="color:red"></span>

    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>